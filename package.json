{
  "name": "daysave-app",
  "version": "1.1.1",
  "description": "DaySave - Save Your Social Media Moments",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:setup": "node scripts/setup-db.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "build": "npm run lint && npm test",
    "deploy": "gcloud app deploy",
    "logo:optimize": "node scripts/optimize-images.js"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-rate-limit": "^6.11.2",
    "express-session": "^1.18.1",
    "express-validator": "^6.15.0",
    "fingerprintjs2": "^2.1.4",
    "helmet": "^7.2.0",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.1",
    "nodemailer": "^6.10.1",
    "passport": "^0.6.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-microsoft": "^1.1.0",
    "qrcode": "^1.5.4",
    "sanitize-html": "^2.17.0",
    "sequelize": "^6.37.7",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^8.43.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "sharp": "^0.32.6"
  }
}
